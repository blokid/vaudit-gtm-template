const setInWindow = require('setInWindow'); // Use this API
const makeString = require('makeString');
const logToConsole = require('logToConsole');

// 2. Get the value from the 'siteIdentifier' field
const siteId = makeString(data.siteId);

// 3. Define the CDN URL for your pixel script
const PIXEL_SCRIPT_URL = 'https://pixel.vaudit.com/static/js/pixel.js';

// 4. Set the window variable directly.
//    This is the linter-safe way to create window.vauditTracker

setInWindow('vauditTracker', { siteIdentifier: siteId });

// 5. Inject your main pixel script from your CDN.
injectScript(PIXEL_SCRIPT_URL, data.gtmOnSuccess, data.gtmOnFailure, 'vaudit-pixel');


